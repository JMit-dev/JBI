<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.ToggleButton?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane prefHeight="400.0" prefWidth="602.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.jbi.controller.RePlanQueueController">
   <children>
      <TableView fx:id="table" layoutX="1.0" layoutY="34.0" prefHeight="360.0" prefWidth="600.0" AnchorPane.bottomAnchor="1.0" AnchorPane.leftAnchor="1.0" AnchorPane.rightAnchor="1.0" AnchorPane.topAnchor="39.0">
        <columns>
            <TableColumn fx:id="idxCol" prefWidth="75.0" text="Index" />
          <TableColumn fx:id="typeCol" prefWidth="75.0" text="Type" />
            <TableColumn fx:id="typeCol1" prefWidth="75.0" text="Name" />
          <TableColumn fx:id="paramCol" prefWidth="75.0" text="Parameters" />
            <TableColumn fx:id="userCol" prefWidth="75.0" text="USER" />
            <TableColumn fx:id="grpCol" prefWidth="75.0" text="GROUP" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <HBox alignment="CENTER_LEFT" prefHeight="34.0" prefWidth="600.0" spacing="10.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
         <children>
            <Label text="QUEUE" HBox.hgrow="ALWAYS" />
            <HBox alignment="CENTER" spacing="5.0" HBox.hgrow="ALWAYS">
               <children>
                  <Button fx:id="upBtn" mnemonicParsing="false" text="Up" />
                  <Button fx:id="downBtn" mnemonicParsing="false" text="Down" />
                  <Button fx:id="topBtn" mnemonicParsing="false" text="Top" />
                  <Button fx:id="bottomBtn" mnemonicParsing="false" text="Bottom" />
               </children>
            </HBox>
            <HBox alignment="CENTER" spacing="5.0" HBox.hgrow="ALWAYS">
               <children>
                  <Button fx:id="deselectBtn" mnemonicParsing="false" text="Deselect" />
                  <Button fx:id="clearBtn" mnemonicParsing="false" text="Clear" />
               </children>
            </HBox>
            <ToggleButton fx:id="loopBtn" mnemonicParsing="false" text="Loop" HBox.hgrow="ALWAYS" />
            <HBox alignment="CENTER_RIGHT" spacing="5.0" HBox.hgrow="ALWAYS">
               <children>
                  <Button fx:id="deleteBtn" mnemonicParsing="false" text="Delete" />
                  <Button fx:id="duplicateBtn" mnemonicParsing="false" text="Duplicate" />
               </children>
            </HBox>
         </children>
      </HBox>
   </children>
</AnchorPane>
